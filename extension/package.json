{
  "name": "teamsync",
  "displayName": "TeamSync",
  "description": "Share Cursor settings with your team",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": ["Other"],
  "activationEvents": [
    "onCommand:teamsync.login",
    "onCommand:teamsync.pushSettings", 
    "onCommand:teamsync.pullSettings"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "teamsync.login",
        "title": "TeamSync: Login"
      },
      {
        "command": "teamsync.pushSettings",
        "title": "TeamSync: Push Settings"
      },
      {
        "command": "teamsync.pullSettings",
        "title": "TeamSync: Pull Settings"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "typescript": "^4.9.4"
  }
}
